!!!
%html
  %head
    %title CashTracker
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
%body
  %header.navbar.navbar-fixed-top
    .navbar-inner
      .container
        = link_to "Home", root_path, id:"logo"
        %nav
          %ul.nav.pull-right
            = link_to "About Us", "#"
            = sign_in_out_link
  .container-fluid
    .container-fluid
      = yield
